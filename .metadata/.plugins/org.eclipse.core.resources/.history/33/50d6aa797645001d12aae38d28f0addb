package thread;

public class D {

	
	int balance ;
	
	public synchronized void add() {
		for(int i = 0 ; i<100 ; i++) {
			balance +=i;
		}
	}
	
	public synchronized void sub() {
		for(int i = 0 ; i<100 ; i++) {
			balance -=i;
		}
	}
	
	public static void main(String[] args) {
		
		A a  = new A();
		
	}
	
}
